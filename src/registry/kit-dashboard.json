{
  "module": "decantr/kit/dashboard",
  "description": "dashboard domain kit",
  "components": {
    "ActivityFeed": {
      "props": {
        "title": {
          "type": "string"
        },
        "items": {
          "type": "Array<{user: string, action: string, time: string, avatar?: string}>"
        },
        "count": {
          "type": "number"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "ChartPlaceholder": {
      "props": {
        "title": {
          "type": "string"
        },
        "height": {
          "type": "string"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "DashboardHeader": {
      "props": {
        "title": {
          "type": "string"
        },
        "search": {
          "type": "boolean"
        },
        "notifications": {
          "type": "boolean"
        },
        "userMenu": {
          "type": "boolean"
        },
        "class": {
          "type": "string"
        }
      },
      "children": "...Node",
      "returns": "HTMLElement"
    },
    "DataTable": {
      "props": {
        "columns": {
          "type": "Array<{key: string, label: string, width?: string}>"
        },
        "data": {
          "type": "Array<Object>|Function",
          "reactive": true
        },
        "searchable": {
          "type": "boolean"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "KPICard": {
      "props": {
        "title": {
          "type": "string"
        },
        "value": {
          "type": "string|number|Function",
          "reactive": true
        },
        "change": {
          "type": "string"
        },
        "icon": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "Sidebar": {
      "props": {
        "nav": {
          "type": "Array<{href: string, label: string, icon?: string}>"
        },
        "branding": {
          "type": "string"
        },
        "collapsible": {
          "type": "boolean"
        },
        "collapsed": {
          "type": "boolean|Function",
          "reactive": true
        },
        "class": {
          "type": "string"
        }
      },
      "children": "...Node",
      "returns": "HTMLElement"
    },
    "StatsGrid": {
      "props": {
        "columns": {
          "type": "number"
        },
        "items": {
          "type": "Array<{title: string, value: string|number|Function, change?: string, icon?: string, status?: string}>",
          "reactive": true
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    }
  }
}
