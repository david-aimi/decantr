{
  "module": "decantr/kit/content",
  "description": "content domain kit",
  "components": {
    "ArticleLayout": {
      "props": {
        "title": {
          "type": "string|Function",
          "reactive": true
        },
        "author": {
          "type": "Object"
        },
        "date": {
          "type": "string|Function",
          "reactive": true
        },
        "content": {
          "type": "Node|string"
        },
        "toc": {
          "type": "Array<{id: string, text: string, level: number}>"
        },
        "class": {
          "type": "string"
        }
      },
      "children": "...Node",
      "returns": "HTMLElement"
    },
    "AuthorCard": {
      "props": {
        "name": {
          "type": "string|Function",
          "reactive": true
        },
        "avatar": {
          "type": "string|Function",
          "reactive": true
        },
        "bio": {
          "type": "string|Function",
          "reactive": true
        },
        "social": {
          "type": "Array<{href: string, label: string}>"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "CategoryNav": {
      "props": {
        "categories": {
          "type": "Array<{id: string, label: string, count?: number}>"
        },
        "active": {
          "type": "string|Function",
          "reactive": true
        },
        "onChange": {
          "type": "Function"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "PostList": {
      "props": {
        "posts": {
          "type": "Array<{title: string, excerpt: string, date: string, href: string, author: string, image: string}>"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "TableOfContents": {
      "props": {
        "headings": {
          "type": "Array<{id: string, text: string, level: number}>"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    }
  }
}
