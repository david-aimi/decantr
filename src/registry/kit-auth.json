{
  "module": "decantr/kit/auth",
  "description": "auth domain kit",
  "components": {
    "AuthLayout": {
      "props": {
        "class": {
          "type": "string"
        }
      },
      "children": "...(string|Node)",
      "returns": "HTMLElement"
    },
    "ForgotPasswordForm": {
      "props": {
        "onSubmit": {
          "type": "Function"
        },
        "logo": {
          "type": "string|Node"
        },
        "title": {
          "type": "string"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "LoginForm": {
      "props": {
        "onSubmit": {
          "type": "Function"
        },
        "logo": {
          "type": "string|Node"
        },
        "title": {
          "type": "string"
        },
        "showRemember": {
          "type": "boolean"
        },
        "showForgot": {
          "type": "boolean"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    },
    "RegisterForm": {
      "props": {
        "onSubmit": {
          "type": "Function"
        },
        "fields": {
          "type": "Array<{name:string, label:string, type:string, placeholder?:string}>"
        },
        "showTerms": {
          "type": "boolean"
        },
        "title": {
          "type": "string"
        },
        "class": {
          "type": "string"
        }
      },
      "returns": "HTMLElement"
    }
  }
}
