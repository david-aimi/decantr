    { "path": "/", "page": "home", "label": "Home" },
    { "path": "/pricing", "page": "pricing", "label": "Pricing" },
    { "path": "/about", "page": "about", "label": "About" },
    { "path": "/contact", "page": "contact", "label": "Contact" }
